<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="kr.co.mlec.Contact.ContactDAO">
	
	<insert id="insertMessage" parameterType="contact">
		insert into test_db.contact(name, email, message)
		 values (#{name}, #{email}, #{message})
	</insert>
</mapper>